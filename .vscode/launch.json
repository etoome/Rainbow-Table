    "version": "0.2.0",
    "configurations": [
        {
            "name": "crack",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/crack/debug",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/crack",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build_crack"
        },
        {
            "name": "generator",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/generator/debug",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/generator",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "build_generator"
        }
    ]
}